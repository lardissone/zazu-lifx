{
  "name": "zazu-lifx",
  "version": "1.0.0",
  "description": "Zazu plugin to control your LIFX lights.",
  "blocks": {
    "input": [
      {
        "id": "lifx",
        "type": "PrefixScript",
        "prefix": "lifx",
        "space": true,
        "args": "Required",
        "script": "src/index.js",
        "connections": [
          "Run"
        ]
      }
    ],
    "output": [
      {
        "id": "Copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}
